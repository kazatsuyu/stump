{
  "name": "@stump/workspace",
  "version": "0.1.0",
  "description": "Strong type utilities for metaprogramming",
  "repository": "https://github.com/kazatsuyu/stump/",
  "author": "Shirayama Kazatsuyu <shirayama.kazatsuyu@gmail.com>",
  "license": "MIT OR Apache-2.0",
  "private": "true",
  "workspaces": [
    "packages/base",
    "packages/calc",
    "packages/stump",
